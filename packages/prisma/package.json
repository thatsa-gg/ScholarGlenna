{
    "name": "@glenna/prisma",
    "version": "0.0.1",
    "type": "module",
    "private": true,
    "main": "dist/index.js",
    "types": "src/index.ts",
    "devDependencies": {
        "@rushstack/heft": "0.50.0",
        "typescript": "5.1.6",
        "prisma": "4.16.2",
        "@prisma/client": "4.16.2",
        "@types/node": "20.3.3"
    },
    "dependencies": {
        "@js-temporal/polyfill": "0.4.4",
        "zod": "3.21.4",
        "@glenna/util": "workspace:*",
        "@discordjs/rest": "1.7.1",
        "discord-api-types": "0.37.48"
